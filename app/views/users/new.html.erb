<h2>Sign Up</h2>

<%= form_for :user, url: '/users' do |f| %>
  <% #if @user.errors.any? %>
    <%# @user.errors.full_messages.each do |msg| %>
      <!-- <p><%#= msg %></p> -->
    <%# end %>
  <%# end %>

  <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </p>

  <p>
    <%= f.submit "Sign Up" %>
  </p>

<% end %>